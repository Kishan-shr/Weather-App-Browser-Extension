
{
    "manifest_version": 3,
    "name": "Weather App",
    "description": "Weather App.",
    "version": "1.0",
    "permissions": ["activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png",
        "48": "favicon-48x48.png",
        "128": "favicon-128x128.png"
      }
    },
    "content_scripts": [
      {
        "css": ["style.css"],
        "js": ["script.js"],
        "config": ["config.js"],
        "matches": ["<all_urls>"],
        "run_at": "document_end"
      }
    ],
    "icons": {
      "16": "favicon-16x16.png",
      "48": "favicon-48x48.png",
      "128": "favicon-128x128.png"
    }
  }
  